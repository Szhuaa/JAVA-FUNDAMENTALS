package com.mycompany.loops;

/**
 *
 * @author Admin
 */
public class Loops {
    public static void main(String[] args) {
        for (int i = 0; i < 5; i++) {
            System.out.println("For Loop: " + i);
        }

        int j = 0;
        while (j < 5) {
            System.out.println("While Loop: " + j);
            j++;
        }
    }
}
    

